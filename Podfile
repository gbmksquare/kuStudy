workspace 'kuStudy'
xcodeproj 'kuStudy/kuStudy.xcodeproj/'

inhibit_all_warnings!

use_frameworks!

def shared_pods
	pod 'Alamofire'
	pod 'RealmSwift'
	pod 'SwiftyJSON'
	pod 'KeychainAccess'
end

target 'kuStudy' do
	shared_pods
end

target 'kuStudyKit' do
	shared_pods
end

target 'kuStudy Today Extension' do
	shared_pods
end

target 'kuStudy WatchKit Extension' do
	shared_pods
end
